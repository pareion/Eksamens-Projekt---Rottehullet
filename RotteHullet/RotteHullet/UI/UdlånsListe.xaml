<Window x:Class="RotteHullet.UI.UdlånsListe"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RotteHullet.UI"
        mc:Ignorable="d"
        Title="UdlånsListe" Height="700" Width="680">
    <Grid>
        <ListView x:Name="lv_Udlånsliste" HorizontalAlignment="Left" Height="586" VerticalAlignment="Top" Width="621" Margin="23,10,0,0">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Aktiv" Width="200" DisplayMemberBinding="{Binding Medlem.Fornavn }" />
                    <GridViewColumn Header="Afleveres" Width="200" DisplayMemberBinding="{Binding Medlem.Fornavn }" />
                    <GridViewColumn Header="Aktiv type" Width="200" DisplayMemberBinding="{Binding Medlem.Fornavn }" />
                    <GridViewColumn/>

                </GridView>
            </ListView.View>
        </ListView>
        <Button x:Name="btn_Luk" Content="Luk" HorizontalAlignment="Left" Margin="304,618,0,0" VerticalAlignment="Top" Width="76" Click="btn_Luk_Click"/>

    </Grid>
</Window>
