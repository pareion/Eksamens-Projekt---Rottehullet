<Window x:Class="RotteHullet.UdlånPopup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RotteHullet"
        mc:Ignorable="d"
        Title="Udlåning" Height="500" Width="400">
    <Grid>
        <Label x:Name="label" Content="Forventning af aflevering" HorizontalAlignment="Left" Margin="10,31,0,0" VerticalAlignment="Top"/>
        <DatePicker x:Name="dp_aflevering" HorizontalAlignment="Left" Margin="10,57,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.272,0.524"/>
        <ListView x:Name="listView" HorizontalAlignment="Left" Height="279" Margin="10,101,0,0" VerticalAlignment="Top" Width="372">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
        <Button x:Name="btn_Acceptere" Content="Bekræft" HorizontalAlignment="Left" Margin="55,399,0,0" VerticalAlignment="Top" Padding="10,5"/>
        <Button x:Name="btn_Annullere" Content="Annullere" HorizontalAlignment="Left" Margin="250,399,0,0" VerticalAlignment="Top" Padding="10,5"/>
    </Grid>
</Window>
