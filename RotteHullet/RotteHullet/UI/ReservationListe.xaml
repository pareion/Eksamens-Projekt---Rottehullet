<Window x:Class="RotteHullet.ReservationListe"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RotteHullet"
        mc:Ignorable="d"
        Title="ReservationListe" Height="500" Width="400">
    <Grid>
        
        <ListView x:Name="listView" HorizontalAlignment="Stretch" Margin="10,50,10,10" VerticalAlignment="Stretch" Width="Auto" Height="Auto">
            <ListView.View>
                <GridView>
                    <GridViewColumn Header="Type" DisplayMemberBinding="{Binding Type}"/>
                </GridView>
            </ListView.View>
        </ListView>
        <Button x:Name="btn_Fjern" Content="Fjern" HorizontalAlignment="Right" Margin="0,21,70,0" VerticalAlignment="Top" Padding="5,2" Click="Fjern_Click"/>
        <Button x:Name="btn_FjernAlle" Content="Slet alle" HorizontalAlignment="Right" Margin="0,21,10,0" VerticalAlignment="Top" Padding="5,2" Click="SletAlle_Click"/>
        <Button x:Name="btn_Reservere" Content="Reservere" HorizontalAlignment="Right" Margin="0,21,317,0" VerticalAlignment="Top" Padding="5,2" Click="Reservere_Click"/>

    </Grid>
</Window>
